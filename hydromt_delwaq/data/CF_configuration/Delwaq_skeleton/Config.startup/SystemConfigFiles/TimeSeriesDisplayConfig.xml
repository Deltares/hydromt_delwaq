<?xml version="1.0" encoding="UTF-8"?>
<timeSeriesDisplay xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/timeSeriesDisplay.xsd">
	<generalDisplayConfig>
		<convertDatum>true</convertDatum>
	</generalDisplayConfig>
	<defaultViewPeriod unit="day" start="-30" end="30"/>
	<classBreaks>
		<classBreaks id="Class.Discharge">
			<break color="FFFFFF" opaquenessPercentage="10" symbolSize="1" lowerValue="0" label=">=0"/>
			<break color="E2E2E2" opaquenessPercentage="80" symbolSize="1" lowerValue="0.5" label=">=0.5"/>
			<break color="DED783" opaquenessPercentage="80" symbolSize="2" lowerValue="1.0" label=">=1.0"/>
			<break color="9BC856" opaquenessPercentage="80" symbolSize="3" lowerValue="2.5" label=">=2.5"/>
			<break color="00C9A6" opaquenessPercentage="80" symbolSize="4" lowerValue="5.0" label=">=5.0"/>
			<break color="00BAC3" opaquenessPercentage="80" symbolSize="6" lowerValue="7.5" label=">=7.5"/>
			<break color="008EC4" opaquenessPercentage="80" symbolSize="8" lowerValue="10.0" label=">=10.0"/>
			<break color="0065CA" opaquenessPercentage="80" symbolSize="10" lowerValue="25.0" label=">=25.0"/>
			<break color="0040FE" opaquenessPercentage="80" symbolSize="15" lowerValue="50.0" label=">=50.0"/>
			<break color="0022AA" opaquenessPercentage="80" symbolSize="20" lowerValue="100.0" label=">=100.0"/>
		</classBreaks>
		<classBreaks id="Class.Discharge.AbsDiff">
			<break color="00C9A6" opaquenessPercentage="80" symbolSize="10" lowerValue="-5.0" label=">=-5.0"/>
			<break color="9BC856" opaquenessPercentage="80" symbolSize="8" lowerValue="-2.5" label=">=-2.5"/>
			<break color="9BC856" opaquenessPercentage="80" symbolSize="6" lowerValue="-1.0" label=">=-1.0"/>
			<break color="DED783" opaquenessPercentage="80" symbolSize="6" lowerValue="-0.5" label=">=-0.5"/>
			<break color="DED783" opaquenessPercentage="50" symbolSize="6" lowerValue="-0.25" label=">=-0.25"/>
			<break color="FFFFFF" opaquenessPercentage="10" symbolSize="1" lowerValue="0" label=">=0"/>
			<break color="DED783" opaquenessPercentage="50" symbolSize="6" lowerValue="0.25" label=">=0.25"/>
			<break color="DED783" opaquenessPercentage="80" symbolSize="6" lowerValue="0.5" label=">=0.5"/>
			<break color="9BC856" opaquenessPercentage="80" symbolSize="6" lowerValue="1.0" label=">=1.0"/>
			<break color="9BC856" opaquenessPercentage="80" symbolSize="8" lowerValue="2.5" label=">=2.5"/>
			<break color="00C9A6" opaquenessPercentage="80" symbolSize="10" lowerValue="5.0" label=">=5.0"/>
			<break color="00BAC3" opaquenessPercentage="80" symbolSize="12" lowerValue="7.5" label=">=7.5"/>
			<break color="008EC4" opaquenessPercentage="80" symbolSize="14" lowerValue="10.0" label=">=10.0"/>
			<break color="0065CA" opaquenessPercentage="80" symbolSize="16" lowerValue="25.0" label=">=25.0"/>
			<break color="0040FE" opaquenessPercentage="80" symbolSize="18" lowerValue="50.0" label=">=50.0"/>
			<break color="0022AA" opaquenessPercentage="80" symbolSize="20" lowerValue="100.0" label=">=100.0"/>
		</classBreaks>
		<classBreaks id="Class.Discharge.RelDiff">
			<break color="00C9A6" opaquenessPercentage="80" symbolSize="10" lowerValue="-0.50" label=">=-0.5"/>
			<break color="9BC856" opaquenessPercentage="80" symbolSize="8" lowerValue="-0.25" label=">=-0.25"/>
			<break color="9BC856" opaquenessPercentage="80" symbolSize="6" lowerValue="-0.10" label=">=-0.10"/>
			<break color="DED783" opaquenessPercentage="80" symbolSize="6" lowerValue="-0.05" label=">=-0.05"/>
			<break color="DED783" opaquenessPercentage="50" symbolSize="6" lowerValue="-0.025" label=">=-0.025"/>
			<break color="FFFFFF" opaquenessPercentage="10" symbolSize="1" lowerValue="0" label=">=0"/>
			<break color="DED783" opaquenessPercentage="50" symbolSize="6" lowerValue="0.025" label=">=0.025"/>
			<break color="DED783" opaquenessPercentage="80" symbolSize="6" lowerValue="0.05" label=">=0.05"/>
			<break color="9BC856" opaquenessPercentage="80" symbolSize="6" lowerValue="0.10" label=">=0.10"/>
			<break color="9BC856" opaquenessPercentage="80" symbolSize="8" lowerValue="0.25" label=">=0.25"/>
			<break color="00C9A6" opaquenessPercentage="80" symbolSize="10" lowerValue="0.50" label=">=0.50"/>
			<break color="00BAC3" opaquenessPercentage="80" symbolSize="12" lowerValue="0.75" label=">=0.75"/>
			<break color="008EC4" opaquenessPercentage="80" symbolSize="14" lowerValue="1.0" label=">=1.0"/>
			<break color="0065CA" opaquenessPercentage="80" symbolSize="16" lowerValue="2.5" label=">=2.50"/>
			<break color="0040FE" opaquenessPercentage="80" symbolSize="18" lowerValue="5" label=">=5.0"/>
			<break color="0022AA" opaquenessPercentage="80" symbolSize="20" lowerValue="10" label=">=10"/>
		</classBreaks>

		<classBreaks id="Class.Demand">
			<break color="FFFFFF" lowerValue="0" label=">=0" opaquenessPercentage="10"/>
			<break color="E2E2E2" lowerValue="0.1" label=">=0.1" opaquenessPercentage="80"/>
			<break color="DED783" lowerValue="0.2" label=">=0.2" opaquenessPercentage="80"/>
			<break color="9BC856" lowerValue="0.3" label=">=0.3" opaquenessPercentage="80"/>
			<break color="00C9A6" lowerValue="0.4" label=">=0.4" opaquenessPercentage="80"/>
			<break color="00BAC3" lowerValue="0.5" label=">=0.5" opaquenessPercentage="80"/>
			<break color="008EC4" lowerValue="1" label=">=1" opaquenessPercentage="80"/>
			<break color="0065CA" lowerValue="2.5" label=">=2.5" opaquenessPercentage="80"/>
			<break color="0040FE" lowerValue="5" label=">=5" opaquenessPercentage="80"/>
			<break color="0022AA" lowerValue="10" label=">=10" opaquenessPercentage="80"/>
		</classBreaks>
	</classBreaks>
	<timeMarkersDisplayConfig>
		<timeMarkerDisplayOptions marker="systemTime">
			<color>green</color>
			<lineStyle>solid</lineStyle>
			<label>System Time</label>
		</timeMarkerDisplayOptions>
		<timeMarkerDisplayOptions marker="timeZero">
			<color>gray</color>
			<lineStyle>dotted</lineStyle>
			<label>T0</label>
		</timeMarkerDisplayOptions>
		<timeMarkerDisplayOptions marker="displayTime">
			<color>red</color>
			<lineStyle>solid</lineStyle>
			<label>Display Time</label>
		</timeMarkerDisplayOptions>
		<timeMarkerDisplayOptions marker="threshold">
			<color>black</color>
			<lineStyle>dashed</lineStyle>
			<label>Threshold</label>
		</timeMarkerDisplayOptions>
	</timeMarkersDisplayConfig>
	<parametersDisplayConfig>
		<parameterDisplayOptions id="Q.obs">
			<preferredColor>blue</preferredColor>
			<lineStyle>solid</lineStyle>
		</parameterDisplayOptions>
	</parametersDisplayConfig>
	<statisticalFunctions>
		<statisticalFunction function="differences"/>
		<statisticalFunction function="accumulationAggregation" ignoreMissings="true">
			<movingAccumulationTimeSpan unit="day" multiplier="1"/>
			<movingAccumulationTimeSpan unit="day" multiplier="31"/>
			<movingAccumulationTimeSpan unit="day" multiplier="365"/>
		</statisticalFunction>
		<statisticalFunction function="movingAverage" ignoreMissings="true">
			<movingAccumulationTimeSpan unit="day" multiplier="1"/>
			<movingAccumulationTimeSpan unit="week" multiplier="1"/>
			<movingAccumulationTimeSpan unit="week" multiplier="2"/>
			<movingAccumulationTimeSpan unit="week" multiplier="4"/>
			<movingAccumulationTimeSpan unit="week" multiplier="12"/>
			<movingAccumulationTimeSpan unit="day" multiplier="365"/>
		</statisticalFunction>
		<statisticalFunction function="historicalAnalysis">
			<historicalPeriods>
				<historicalPeriod>
					<startForwardLookingPeriod>--04-01</startForwardLookingPeriod>
					<season startMonthDay="--04-01" endMonthDay="--10-01" label="zomer"/>
				</historicalPeriod>
			</historicalPeriods>
		</statisticalFunction>
		<statisticalFunction function="accumulationInterval" ignoreMissings="true"/>
		<statisticalFunction function="durationExceedence"/>
		<statisticalFunction function="showLowsBelow"/>
		<statisticalFunction function="showPeaksAbove"/>
		<statisticalFunction function="frequencyDistribution">
			<samples amount="5"/>
			<samples amount="10"/>
			<samples amount="20"/>
		</statisticalFunction>
		<statisticalFunction function="cumulative"/>
		<statisticalFunction function="scatterPlot"/>
	</statisticalFunctions>
	<descriptiveFunctionGroups>
		<descriptiveFunctionGroup name="Descriptive statistics">
			<descriptiveFunction function="mean" ignoreMissings="true"/>
			<descriptiveFunction function="min"/>
			<descriptiveFunction function="max"/>
			<descriptiveFunction function="sum" ignoreMissings="true"/>
			<descriptiveFunction function="standardDeviation" ignoreMissings="true"/>
			<descriptiveFunction function="percentileExceedence" ignoreMissings="true">
				<value>5</value>
				<value>25</value>
				<value>50</value>
				<value>75</value>
				<value>95</value>
			</descriptiveFunction>
		</descriptiveFunctionGroup>
		<descriptiveFunctionGroup name="Time series info">
			<descriptiveFunction function="infoLocationId"/>
			<descriptiveFunction function="infoLocationName"/>
			<descriptiveFunction function="infoParameterId"/>
			<descriptiveFunction function="infoParameterName"/>
			<descriptiveFunction function="infoModuleinstance"/>
			<descriptiveFunction function="startTime"/>
			<descriptiveFunction function="endTime"/>
		</descriptiveFunctionGroup>
		<descriptiveFunctionGroup name="Data quality">
			<descriptiveFunction function="count"/>
			<descriptiveFunction function="completed"/>
			<descriptiveFunction function="corrected"/>
			<descriptiveFunction function="reliables"/>
			<descriptiveFunction function="doubtfuls"/>
			<descriptiveFunction function="unreliables"/>
			<descriptiveFunction function="missings"/>
		</descriptiveFunctionGroup>
	</descriptiveFunctionGroups>
</timeSeriesDisplay>
